.regsucpost{padding-top:20px; clear:both; width:670px; margin:0 auto;}
.regsucpost li{float:left; width:150px; height:80px; padding-right:17px;}
.regsucpost li a.button{padding:12px 35px;}
.regsucpost li span{height:25px; line-height:25px; display:block; padding-top:15px;}


/*弹出层登陆*/


.wrapper { background:#fff; }

.word_login p { color:#666; }

.word_login dl { padding:0; }

.word_login dl dt { background:url(../images/login/slogin.png) no-repeat; display:block; float:left; height:16px; margin-bottom:10px; width:16px; }

.word_login dl dd { float:left; height:16px; line-height:16px; margin-bottom:10px; margin-left:5px; margin-right:10px; }

.reflect { margin-top:1px; }

label.logintxt { position:absolute; left:75px; .left:70px;top:9px; color:#bdbdbd; -moz-user-select: none; cursor:text; }

.tipsy-inner { _width:200px; }

.button_box { padding-left: 46px; }

.button_box .submit, .button_box a.button { font-size:14px; }

.button_box .submit { width: 100px; height:28px; line-height:16px; }

.button_box a.button { line-height:16px; height: 18px; width: 80px; text-align:center; }

.winbody .button_box, .winbody .tips_box { padding-left:30px; background:#fff; }

.winbody .left_side { width:310px; padding-left:20px; float:left; }

.winbody .word_login { float:left; width:190px; padding-left:15px; border-left:1px solid #eee; overflow:hidden; }

.winbody .word_login dl dd { margin-right:20px; width:50px; }

.error_box { position:absolute; right:10px; top:22px; background:#FFE6C6; color:#333; }

.error_box dl { border:1px solid #FFC67C; background:#FFFCE4; position:relative; top:-1px; left:-1px; padding:10px 15px 0; z-index:7; }

.error_box dt i, .l_box .error p i { display:inline-block; width:16px; height:16px; background:url(../images/login/error.gif) no-repeat 0 center; margin-right:5px; vertical-align:-3px; *vertical-align:-1px;
}

.error_box dt strong { font-size:14px; color:#BF3A00; }

.error_box dd { margin-bottom:18px; }

.error_box dd a { text-decoration:underline; }

.error_box .close { display:block; width:11px; height:10px; overflow:hidden; background:url(../images/login/error_ico.png) no-repeat 0 1px; position:absolute; right:12px; top:12px; z-index:9; }

.error_box .close:hover { background-position:0 -9px; }

.error_box .arrowr { display:block; position:absolute; right:-13px; top:50%; width:15px; height:29px; margin-top:-15px; background:url(../images/login/error_ico.png) no-repeat 0 -20px; z-index:8; }

.valid_error span { display: inline-block; vertical-align:middle; overflow: hidden; }

.loginwaitbox { width:16px; display: inline-block; overflow: hidden; vertical-align: middle; }

.bwhite{height:26px; line-height:26px ; background:#f4f4f4; border:1px solid #e9e9e9; width:80px; display:inline-block; text-align:center; padding:0 5px;}

/*开发师登陆*/
.kfleft{background:url(../images/login/kflog_r2_c1_s1.gif) no-repeat 0 160px; width:600px; padding:0 50px; float:left;}
.kfleft_li{background:#fff; padding:40px 0 40px 40px;}
.kfleft_li h1 { height:40px; line-height:40px; font-size:30px; font-family:"微软雅黑"; font-weight:500; padding-bottom:40px; }
.kfleft_li h1 span { font-size:14px; padding-left:15px; padding-top:20px; line-height:30px; font-weight:normal; }
.kfleft_li li{clear:both; height:40px; padding-bottom:10px;}
.kfleft_li li u{height:40px; line-height:40px; width:80px; text-align:right; display:inline-block;}
.kfleft_li li em{height:40px; width:268px;display:inline-block; text-align:left;}
.kfleft_li li em input{height:18px; line-height:18px; padding:10px 3px; width:254px;}
.kfleft_li li em input.sj{width:154px;}
.kfleft_li li em input.yq{width:124px;}
.kfleft_li li em.xy{vertical-align:middle; line-height:40px;}
.kfleft_li li em.xy input{width:auto; vertical-align:middle; margin-right:5px;}

.kfleft_li li.btn{height:50px; padding-top:10px; padding-left:80px; padding-bottom:20px;}
.kfleft_li li.btn button{background:#2DA893; border-color:#2DA893; width:200px; height:50px; line-height:40px; padding:0; color:#FFF; font-family:"微软雅黑"; font-size:25px;filter:chroma(color=#000000)}
.kfleft_li li .codebtn{margin-left:5px;width:100px; height:40px; line-height:40px; padding:0; background:#2CA792; text-align:center; color:#FFF; border:none;}
.kfleft_li .fontcol{color:#2CA792;}

.kfright{background:url(../images/login/kflog_r2_c2_s1.gif) no-repeat 0 160px #fff; width:500px; float:left;}
.kfshoux{background:url(../images/login/kflog_r1_c2_s1.gif) no-repeat; height:160px; padding-left:190px;}
.kfshoux h3{font-family:"微软雅黑"; font-size:25px; line-height:40px; height:40px; padding-top:25px;}
.kfshoux a.button{width:140px; height:40px; line-height:40px; text-align:center; font-size:15px; font-family:"微软雅黑"; padding:0; margin-top:15px;}
.kfrig_txt{height:300px;}
.kfrig_txt ul{height:80px; padding-left:100px; color:#fff; padding-top:20px;}
.kfrig_txt ol{font-family:"微软雅黑"; font-size:25px; font-weight:bold; height:35px; line-height:35px;}
.kfrig_txt li{height:25px; line-height:25px;}

.kfico{padding:30px 0 20px 20px;}




