.regusersucc_li { border-top:1px dashed #dfdfdf; padding-top:20px; }

.regusersucc_li ul { float:left; width:254px; height:170px; border:1px solid #dfdfdf; background:#f4f4f4; padding:15px; margin-left:20px; }

.regusersucc_li ul.ml_on { margin-left:0; }

.regusersucc_li ul span { height:30px; line-height:30px; font-size:15px; font-weight:bold; font-family:"微软雅黑"; display:block; padding-bottom:10px; }

.regusersucc_li ul samp { height:40px; line-height:20px; font-size:14px; display:block; padding:10px 0; }

