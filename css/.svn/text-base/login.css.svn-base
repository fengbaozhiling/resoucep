@charset "utf-8";

/* CSS Document */
.a2 a:link, .a2 a:visited, .a2 a:active { color:#0066CC; }

.a2 a:hover { color:#003060; }

u, i, em { font-style:normal; text-decoration:none; }

.heigth1 { height:10px; line-height:0px; font-size:0; overflow:hidden; clear:both; }

.heigth2 { height:20px; line-height:0px; font-size:0; overflow:hidden; clear:both; }

.heigth3 { height:30px; line-height:0px; font-size:0; overflow:hidden; clear:both; }

.loginbg { background:url(../images/login/loginbg.jpg) repeat-x 0 0 #f4f4f4; }

.loginptb { padding:60px 0; }

.logleft { width:580px; padding-left:140px;}

.logright { width:300px; padding-right:170px; }

.login { clear:both; border-right:1px solid #dfdfdf; }

.login h1 { font-size:30px; height:40px; line-height:40px; font-family:"微软雅黑"; font-weight:500; padding-bottom:40px; }

.login li {line-height:40px; padding:5px 0; clear:both; }

.login li u { float:left; width:75px; font-size:14px; padding-left:5px; }

.login li em { float:left; width:460px; }

.login li em input { height:18px; line-height:18px; width:260px; border:1px solid #e9e9e9; background:#fff; padding:10px 15px; font-size:14px; color:#999; }

.login li b { font-weight:normal; }

.login li b i { width:13px; height:13px; display:inline-block; background:url(../images/common/arrow_main.png) -50px 0; margin:0 5px -3px 0; cursor:pointer; }

.login li b i.checkbox { background-position:-80px 0; }

.login ol { padding-left:80px; height:50px; line-height:50px; display:block; clear:both; }

.login ol button { background:#ec2828; color:#fff; font-size:28px; text-align:center; font-family:"微软雅黑"; width:200px; height:50px; border:none; line-height:30px; vertical-align:middle;}
.login ol:hover button{background:#fd3939;}

.login ol a { margin-left:15px; }

.login li.logintip{height:24px; line-height:24px; padding:0 0 0 70px;}

.alruser { padding-left:50px; padding-top:80px; }

.alruser h3 { height:50px; line-height:50px; font-size:25px; font-family:"微软雅黑"; display:block; font-weight:500; }

.alruser span { height:50px; display:block; padding-top:15px; }

.alruser span a { height:50px; width:200px; text-align:center; color:#fff; line-height:50px; background:#ec2828; font-size:25px; font-family:"微软雅黑"; display:block; font-weight:500; border-radius:2px;}

.alruser span a:hover { background:#fd3939; }

.alruser samp { height:50px; padding-top:10px; display:block; }

.alruser samp a { background:url(../images/login/hezhuoico.png) no-repeat 0 0; width:30px; height:30px; float:left; display:block; margin-left:10px; }

.alruser samp a.icoqq { margin-left:0; }

.alruser samp a.icosina { background-position:-50px 0; }

.alruser samp a.icodd { background-position:-100px 0; }

.alruser samp a.icorr { background-position:-150px 0; }

.alruser samp a.icotao { background-position:-200px 0; }

.regli { clear:both; border-right:1px solid #dfdfdf; }

.regli h1 { height:40px; line-height:40px; font-size:30px; font-family:"微软雅黑"; font-weight:500; padding-bottom:40px; }

.regli h1 span { font-size:14px; padding-left:15px; padding-top:20px; line-height:30px; font-weight:normal; }

.regli li { height:40px; line-height:40px; padding:5px 0; clear:both; display:block; font-size:14px; }

.regli li u { width:85px; padding-left:5px; float:left; }

.regli li em { float:left; height:40px; display:block; }

.regli li em input { border:1px solid #e9e9e9; padding:10px 15px; height:18px; line-height:18px; width:286px; font-size:16px; }
.regli li em button{background:#FF6600; border:1px solid #D55500;padding:10px 8px; color:#fff;}
.regli li em button.disa, .regli li em button:hover.disa{border:1px solid #cdcdcd; background:#cdcdcd; padding:10px 8px; color:#999; cursor:default;}

.regli li q { height:50px; float:left; display:block; padding-left:10px; }

.regli li q.valid_error { color:#ec2828; }

.regli li q.valid_success font{padding-left:20px; line-height:50px;}

.regli li q span { padding-left:20px; line-height:50px; }

.regli li em.code input { width:155px; float:left; }

.regli li em.code a {float:left;}
.regli li em.code a.refr{ padding-left:5px; height:14px; width:20px; background:url(../images/common/refresh.png) no-repeat 5px -155px; display:inline-block; overflow:hidden; margin-top:18px; filter: Alpha(Opacity=70); Opacity:0.7;}
.regli li em.code a:hover.refr{filter: Alpha(Opacity=100); Opacity:1;}

.regli li em.code img { vertical-align:middle; margin-left:5px; padding-top:8px; float:left; }

.regli li.agreement { padding-left:90px; height:35px; line-height:35px; font-size:12px; }

.regli li.agreement i { width:13px; height:13px; display:inline-block; background:url(../images/common/arrow_main.png) -50px 0; margin:0 5px 0 0; cursor:pointer; }

.regli li.agreement i.checkbox { background-position:-80px 0; }

.regli li.but { padding-left:90px; }

.regli li.but button {width:200px; height:50px; line-height:50px; border:none; color:#fff; text-align:center; font-size:25px; font-family:"微软雅黑"; line-height:18px; background:#ec2828; line-height:25px;}

.regli li.but:hover button{background:#fd3939;}

/*成功*/
.regsucc { padding:50px 0; }

.regsuccbox { background:#fff; border:1px solid #ddd; padding:40px 120px; width:760px; margin:0 auto; }

.regemailok { padding-left:80px; background:url(../images/login/mailbox_ve.gif) no-repeat 10px 50px; }

.regemailok h3 { height:40px; line-height:40px; font-size:22px; font-family:"微软雅黑"; font-weight:500; }

.regemailok li { height:25px; line-height:25px; font-size:14px; }

.regemailok li.btn { padding-top:15px; height:35px; }

.regemailok li.btn a { width:220px; height:35px; line-height:35px; text-align:center; display:block; color:#fff; background:#ec2828; }

.regemailok li.btn a:hover { background-color:#AC0F0F; }

.regemailsucc { border:1px solid #ddd; background:#f4f4f4; padding:40px; font-size:14px; }

.regemailsucc h3 { height:40px; line-height:40px; font-size:18px; font-family:"微软雅黑"; font-weight:500; }

.regemailsucc ul { padding-top:10px; }

.regemailsucc li { line-height:25px; height:25px; }

.regemailsucc span { padding-top:20px; line-height:25px; display:block; }

.regusersuccbox { background:#fff; border:1px solid #ddd; padding:40px 50px; width:900px; margin:0 auto; }

.regusersucc { background:url(../images/login/success.png) no-repeat 10px 35px; padding:20px 0 20px 120px; }

.regusersucc h3 { height:40px; line-height:40px; font-size:24px; font-family:"微软雅黑"; font-weight:500; color:#5da716;}

.regusersucc span { height:35px; line-height:35px; display:block; font-size:14px; }

