@charset "utf-8";
@import url("rigpost.css");

/* CSS Document */


img.imgsize { width:70px; height:70px; }

.left { float:left; width:860px; }

.right { float:right; width:320px; }

#flash { POSITION:relative; z-index:1; MARGIN:0px auto; HEIGHT:350px; CLEAR:both; OVERFLOW:hidden; text-align:center; }

#flash A { DISPLAY:none; HEIGHT:350px; }

#flash IMG { HEIGHT:350px }

.flash_bar { POSITION:absolute; TEXT-ALIGN:center; width:100%; BOTTOM:10px; _BOTTOM:30px;OVERFLOW:hidden; margin-right:auto; margin-left:auto; }
.flash_bar .dq { MARGIN:3px 0; WIDTH:15px; DISPLAY:inline-block; BACKGROUND:#ec2828; border-radius:15px; HEIGHT:15px; CURSOR:pointer; }
.flash_bar .no { MARGIN:3px 0; WIDTH:15px; DISPLAY:inline-block; BACKGROUND:#fff; border-radius:10px; HEIGHT:15px; CURSOR:pointer; }

#flashBg { margin:auto; WIDTH:100%; HEIGHT:350px; CLEAR:both; padding:0px; z-index:1; position:relative; }

#flashLine { WIDTH:100%; HEIGHT:350px; CLEAR:both; }

#flshimgload { position:absolute; width:100%; padding-top:145px; text-align:center; z-index:1; }

#flash A img.imgwait { margin:150px auto auto; width:auto; height:auto; }

#flashBg .container { position:relative; z-index:999; }

.yidueiyi { position:absolute; right:0; top:40px; width:330px; height:230px; z-index:999; background:#fff; padding:20px; }

.yidueiyi h3 { height:50px; line-height:50px; text-align:center; font-size:30px; font-family:"微软雅黑"; font-weight:normal; padding-top:20px; }

.yidueiyi samp { padding:50px 0 20px 0; display:block; }

.yidueiyi a.button { background:#ec2828; color:#fff; font-size:25px; font-family:"微软雅黑"; width:280px; height:50px; line-height:50px; text-align:center; clear:both; display:block; padding:0; margin:0 auto; border-radius:2px; }

.yidueiyi a:hover.button { background:#fd3939; }

.yidueiyi span { height:20px; line-height:20px; display:block; width:280px; margin:0 auto; }

.yidueiyi span a { text-decoration:underline; }

.liuchenico { height:60px; overflow:hidden; }

.liuchenico span { height:60px; line-height:60px; display:block; float:left; width:230px; padding-left:80px; font-family:"微软雅黑"; font-size:14px; background:url(../images/task/liuchenico.png) no-repeat 0 0; white-space:nowrap; overflow:hidden; }

.liuchenico span.liu1 { width:270px; font-size:20px; color:#339999; padding-left:20px; }

.liuchenico span.liu2 { background-position:-290px -0; }

.liuchenico span.liu3 { background-position:-600px -0; }

.liuchenico span.liu4 { background-position:right 0; width:210px; }

.listtitle { background:#f4f4f4; border:1px solid #dfdfdf; border-top:none; height:40px; padding:0 20px; }

.listtitle div { line-height:40px; }

.listtitle div.wrid1 { float:left; width:70px; }

.listtitle div.wrid2 { float:left; width:380px; padding-left:20px; }

.listtitle div.wrid3 { float:left; width:80px; text-align:center; }

.listtitle div.wrid4 { float:left; width:130px; padding-left:20px; }

.listtitle div.wrid5 { float:left; width:110px; text-align:center; }

.tasklist { padding:10px 20px; background:#fff; border:1px solid #e9e9e9; border-top:none; }

.tasklist_li { clear:both; border-bottom:1px solid #e9e9e9; padding:10px 0; color:#666; height:50px; overflow:hidden; }

.tasklist_li span { float:left; height:50px; display:block; }

.tasklist_li span.baomin { border:1px solid #e9e9e9; line-height:50px; width:70px; text-align:center; }

.tasklist_li span.title { width:380px; padding-left:20px; font-size:14px; }

.tasklist_li span.title b { color:#ec2828; height:20px; line-height:20px; display:block; }

.tasklist_li span.title h3 { font-size:14px; font-family:"微软雅黑"; height:30px; line-height:30px; font-weight:normal; }

.tasklist_li span.guzu { width:80px; text-align:center; overflow:hidden;}

.tasklist_li span.addtime { width:130px; padding-left:20px; line-height:25px; }

.tasklist_li span.btn { width:110px; padding-top:10px; height:40px; text-align:center; }

.borbon { border-bottom:none; }

.guzutask { padding:10px 30px; }

.guzutask ul { float:left; width:389px; height:70px; padding:15px 0 15px 10px; }

.guzutask ol { float:left; width:70px; height:70px; }

.guzutask li { float:right; width:310px; height:23px; line-height:23px; overflow:hidden; }

.guzutask li b { color:#ec2828; }

.guzutask span { height:1px; line-height:0; font-size:0; overflow:hidden; width:100%; display:block; clear:both; background:#e9e9e9; }

.zhengqfx { padding:15px 30px; }

.zhengqfx ul { height:100px; padding:15px 0; border-bottom:1px solid #e9e9e9; }

.zhengqfx ul.borbon { border-bottom:none; }

.zhengqfx ol { float:left; width:100px; height:100px; }

.zhengqfx li { float:right; width:680px; }

.zhengqfx li.tit { height:25px; line-height:25px; font-family:"微软雅黑"; font-size:14px; ; font-weight:bold; }

.zhengqfx li.com { height:40px; line-height:20px; margin:7px 0; overflow:hidden;}

.zhengqfx li.addres { height:20px; line-height:20px; }

.guyonzhanbao { padding:20px; }

.guyonzhanbao h3 { font-family:"微软雅黑"; height:30px; line-height:30px; font-size:25px; font-weight:600; padding-bottom:25px; }

.guyonbox { width:270px; height:150px; overflow:hidden; }

.guyonbox .jcarousel-container { width:270px; height:150px; overflow:hidden; }

.guyonbox .jcarousel-clip-horizontal { width:270px; height:150px; }

.guyonbox ul { width:2000px; }

.guyonbox li { float:left; width:270px; height:150px; }

.guyonbox li span { background:#f4f4f4; border:1px solid #e9e9e9; display:block; height:60px; padding:20px; }

.guyonbox li span b { font-size:30px; color:#339999; height:35px; line-height:35px; display:block; font-family:Arial, Helvetica, sans-serif; }

.guyonbox li span u { height:20px; line-height:20px; display:block; }

.guyonbox li samp { height:25px; line-height:25px; display:block; text-align:center; font-size:12px; padding-top:15px; overflow:hidden;}

.jaoyimun { padding:20px 30px; }

.jaoyimun span { height:25px; line-height:25px; display:block; }

.jaoyimun h3 { font-family:Arial, Helvetica, sans-serif; font-size:30px; height:40px; line-height:40px; }

.topfive { background:#339999; height:60px; line-height:60px; font-family:"微软雅黑"; font-size:28px; padding-left:15px; color:#fff; }

.topfivelist { background:#fff; border:1px solid #e9e9e9; border-top:none; padding:20px; }

.topfivelist_li { height:70px; padding:10px 0; border-bottom:1px dashed #e9e9e9; overflow:hidden; }

.topfivelist_li span { float:left; width:70px; height:70px; display:block; position:relative; }

.topfivelist_li span a { width:70px; height:70px; display:block; position:absolute; left:0; top:0; z-index:1; }

.topfivelist_li span i { width:0; height:0; z-index:2; overflow: hidden; font-size: 0; line-height: 0; border-color:#339999 transparent transparent #339999; border-style:solid dashed dashed solid; border-width:14px; display:block; position:absolute; left:0; top:0; }

.topfivelist_li span i.bgon { border-color:#999 transparent transparent #999; }

.topfivelist_li span u { width:15px; height:20px; line-height:20px; color:#fff; display:block; position:absolute; left:0; top:0; z-index:3; text-align:center; }

.topfivelist_li samp { float:right; width:195px; height:70px; display:block; overflow:hidden; }

.topfivelist_li samp h3 { height:22px; line-height:22px; display:block; font-family:"微软雅黑"; font-size:14px; overflow:hidden; }

.topfivelist_li samp u { height:21px; line-height:21px; font-size:12px; display:block; }

.topfivelist_li samp q { height:27px; display:block; font-size:12px; }

.topfivelist_li samp q a.button { padding:5px 15px; }

.borbon { border-bottom:none; }

.zhenqi_ico { padding:20px; }

.zhenqi_ico h3 { font-size:40px; height:60px; line-height:60px; font-family:"微软雅黑"; font-weight:500; display:block; text-align:center; }

.zhenqi_ico u { height:22px; line-height:22px; display:block; color:#999; text-align:center; }

.zhenqi_ico ul { clear:both; height:201px; }

.zhenqi_ico li { float:left; width:138px; height:85px; display:block; border-right:1px solid #e9e9e9; border-bottom:1px solid #e9e9e9; padding-top:15px; text-align:center; line-height:25px; }

.zhenqi_ico li.ron { border-right:none; }

.zhenqi_ico li.bon { border-bottom:none; }

.zhenqi_ico li span { height:50px; display:block; background:url(../images/task/ico_one.png) no-repeat center center; }

.zhenqi_ico li span.bg2 { background-image:url(../images/task/ico_pc.png); }

.zhenqi_ico li span.bg3 { background-image:url(../images/task/ico_wallet.png); }

.zhenqi_ico li span.bg4 { background-image:url(../images/task/ico_bag.png); }

.zhenqi_ico q { height:40px; width:278px; padding-top:15px; display:block; clear:both; float:none; }

.zhenqi_ico q a { background:#ec2828; color:#fff; width:220px; height:40px; line-height:40px; display:block; margin:0 auto; text-align:center; font-family:"微软雅黑"; font-size:16px; border-radius:2px; }

.zhenqi_ico q a:hover { background:#fd3939; }

.rightydy { padding:20px; }

.rightydy h3 { font-family:"微软雅黑"; font-size:28px; font-weight:500; height:60px; line-height:60px; }

.rightydy span { height:50px; display:block; padding:15px 0; }

.rightydy span a { height:50px; line-height:50px; width:260px; margin:0 auto; background:#ec2828; color:#fff; font-size:25px; font-family:"微软雅黑"; text-align:center; display:block; }

.rightydy span a:hover { background:#fd3939; }

.rightydy samp { height:25px; line-height:25px; text-align:center; display:block; font-size:12px; padding-top:10px; }

.rightydy samp a { text-decoration:underline; }

.commendvip { background:#fff; border:1px solid #e9e9e9; border-top:none; padding:15px; }

.commendvip_li { height:70px; padding:15px 0; border-bottom:1px dashed #e9e9e9; clear:both; }

.commendvip_li em { float:left; width:70px; height:70px; display:block; }

.commendvip_li span { float:right; width:210px; height:70px; display:block; }

.commendvip_li span h3 { height:25px; line-height:25px; font-family:"微软雅黑"; font-size:14px; }

.commendvip_li span u { height:18px; line-height:18px; color:#666; display:block; padding:3px 0; }

.commendvip_li span q { height:20px; width:210px; display:block; }

.commendvip_li span q i { float:left; white-space:nowrap; }

.commendvip samp { display:block; height:40px; display:block; padding:15px 0 5px 0; text-align:center; }

.commendvip samp a.button { height:40px; line-height:40px; padding:0 15px; font-size:14px; }

.tale_class { border-bottom:1px dashed #e9e9e9; padding:10px 20px; clear:both; }

.boron { border:none; }

.tale_class b { float:left; width:80px; height:25px; line-height:25px; text-align:right; }

.tale_class span { float:right; width:720px; height:auto; display:block; font-size:13px; font-family:"宋体"; }

.tale_class span a { padding:0 10px; margin:0 8px; height:20px; line-height:20px; display:inline-block; white-space:nowrap; overflow:hidden; float:left; background:#fff; }

.tale_class span a.select { background:#EC2828; color:#fff; }

.tale_class span a:hover { background:#FFECDF; color:#EB2626; }

.talent_sort { padding:20px; clear:both; height:30px; }

.talent_sort span { float:left; height:28px; line-height:28px; display:inline-block; }

.talent_sort span a { background:#f4f4f4; display:inline-block; border:1px solid #e9e9e9; border-right:none; padding:0 12px 0 18px; }

.talent_sort span a.boron { border-right:1px solid #e9e9e9; }

.talent_sort span a:hover { background:#E0E0E0; border-color:#CCC; }

.talent_sort span a.on { background:#fff; }

.talent_sort span a.on, .talent_sort span a.on a:hover { background:none; border-color:#e9e9e9; }

.talent_sort span a b { float:left; font-weight:normal; }

.talent_sort span a i { background:url(../images/task/arrow_page.png) no-repeat 6px 0; width:9px; height:9px; line-height:0; font-size:0; overflow:hidden; display:inline-block; padding-left:6px; float:left; margin-top:9px; }

.talent_sort span a i.uparrow { background-position:6px -30px; }

.talent_sort span a.select { background:#ec2828; border-color:#ad1818; color:#fff; }

.talent_sort span a.select i { background-position:6px -60px; }

.talent_sort span a.select i.uparrow { background-position:6px -90px; }

.talent_sort samp { float:left; height:30px; line-height:30px; display:inline-block; padding:0 4px; }

.talent_sort samp input { width:50px; height:28px; line-height:28px; padding:0 3px; border:1px solid #dfdfdf; }

.talent_sort samp.word { position:relative; }

.talent_sort samp.word input { width:100px; position:absolute; left:5px; top:1px; }

.talent_sort samp.word button { background:url(../images/common/search.png) no-repeat center center; width:20px; height:20px; border:none; position:absolute; left:90px; top:6px; display:none; }

.talent_sort samp.word span { margin-left:5px !important; }

.talent_sort samp.selectbox { width:85px; }

.selectbox div { width:85px; height:30px; background:#fff; border:1px solid #e9e9e9; position:relative; }

.selectbox div em { width:85px; height:30px; line-height:30px; text-indent:10px; cursor:pointer; position:absolute; left:0; top:0; display:block; z-index:4; font-style:normal; background:url(../images/common/arrow_main.png) no-repeat 70px -126px; }

.selectbox div div { display:none; width:550px; height:auto; position:absolute; left:-250px; top:30px; z-index:3; border:1px solid #e9e9e9; background:#fff; padding:10px; }

.selectbox div.select em { border-bottom:1px solid #FFF; background-position:70px -166px; }

.selectbox div.select div { display:block !important; }

.selectbox div.select div a { float:left !important; white-space:nowrap; height:25px !important; line-height:25px; padding:0 12px !important; display:inline-block; overflow:hidden; }

.selectbox div.select div a:hover { background:#FFECDF; }

.talent_sort q { float:right; width:60px; height:30px; display:block; }

.talent_sort q a { background:url(../images/task/arrow_page.png) no-repeat; display:inline-block; width:30px; height:30px; float:right; overflow:hidden; margin:0 0 0 auto; }

.talent_sort q a.prevon { background-position:0 -160px; }

.talent_sort q a.nexton { background-position:right -160px; width:29px; }

.talent_sort q a.prevpage { background-position:0 -130px; }

.talent_sort q a.nextpage { background-position:right -130px; }

.posttask { background:#fff; border:1px solid #e9e9e9; padding:15px 30px; height:70px; }

.posttask ol { font-size:16px; font-weight:bold; line-height:28px; padding-top:12px; }

.posttask li { line-height:20px; color:#999; }

.posttask a { width:210px; height:40px; line-height:40px; text-align:center; color:#fff; font-size:14px; background:#EC2828; font-weight:bold; display:inline-block; margin:15px 20px 0 0; }

.posttask a:hover { margin:17px 20px 0 0; }
