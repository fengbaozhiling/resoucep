@charset "utf-8";
/* CSS Document */
.header {-webkit-box-shadow:0 3px 3px #eaeaea;-moz-box-shadow:0 3px 3px #eaeaea;box-shadow:0 3px 3px #eaeaea; position:relative; z-index:3;}
.wid920 { width:920px;}
.wid260 { width:260px;}
.table_list .coman_table{ width:100%; background:#e9e9e9;}
.table_list .coman_table th{ background:#f4f4f4; text-align:left; padding:10px 20px;}
.table_list .coman_table td{  background:#fff; padding:10px 20px;}


.pay_tab { position:relative; z-index:2; top:1px; }
.pay_tab li { float:left; background:#fff; border:1px solid #efefef; cursor:pointer; padding:15px 50px; margin-right:10px; font-size:16px; }
.pay_tab li.selected { border-bottom:1px solid #fff; background:#fff; font-weight:bold; }
.pay_tab a, .pay_tab li.selected a:hover { color:#333; }
.pay_box h2 { color:#333; }
.pay_box button.button { margin:10px 0 0 0; }
.pay_box .bank_seclet li { float:left;  margin-right:20px; margin-bottom:20px; border:1px solid #efefef; }
.pay_box .bank_seclet li label {padding:10px 14px; width:120px; display:block; cursor:pointer;}
.pay_box .bank_seclet li img { cursor:pointer;}
.pay_box .button,.bigbt button.button,.button.big{ line-height:30px; padding:0 25px;}
#div_paym_2 .table_list .coman_table td{  padding:10px 15px;}

.pay_messages, .pay_bank_messages { background:#FFF2D8 url(../img/style/pay_messages.gif) no-repeat 15px 15px; min-height:80px; _height:expression(this.scrollHeight < 80 ? "80px":"auto");
border:1px solid #FECEAB; }
.pay_bank_messages { background:#FFF2D8 url(../images/common/smile.gif) no-repeat 15px 20px; color:#630; }
.pay_mess_con { padding:13px 0 13px 90px; }
.pay_bank_messages .pay_mess_con { padding:20px 20px 20px 70px; }
.pay_messages .pay_mess_con h2 { color:#333; }
.pay_messages .pay_mess_con span { color:#630; }


.big_title {background:url(../images/release/tabborbg.jpg) repeat-x;height:58px; line-height:58px; border-bottom:1px solid #e9e9e9;}
.big_title strong { font-size:16px; padding:0 20px;}

.chenlist { border:1px solid #e9e9e9; background:#fff; padding-bottom:30px;}
span.ico_cnn { display:inline-block; width:28px; height:28px; background:#f4f4f4; border:1px solid #dfdfdf; text-align:center; line-height:28px; color:#e45c00; font-family:"微软雅黑"; font-size:16px;}
.chenlist li { padding:20px; border-bottom:1px solid #efefef; 	-moz-box-shadow:0 -3px 1px #f4f4f4 inset;
-webkit-box-shadow:0 -3px 1px #f4f4f4 inset; box-shadow:0 -3px 1px #f4f4f4 inset; color:#666;}
.chenlist li  span.ico_cnn { margin-right:15px;}
.chenlist .button { height:28px; line-height:28px; padding:0 35px;}

.atachp { padding:5px 20px; color:#666; line-height:20px;}
.atachp span { margin-right:10px;}
.tac_cio_1 { display:inline-block; width:50px; height:40px; background:url(../images/release/telfphp.png) no-repeat left -5px;}
.tac_cio_2 { display:inline-block; width:50px; height:40px; background:url(../images/release/telfphp.png) no-repeat left -45px;}


.yddownload{border:1px solid #e9e9e9; background:#fff; padding:30px 20px;}
.yddownload ul{height:55px; background:url(../images/task/mobile.jpg) no-repeat 0 center; padding-left:55px;}
.yddownload ul ol{height:25px; line-height:25px; font-size:14px; font-weight:bold;}
.yddownload ul li{height:20px; line-height:20px; color:#999;}
.yddownload b{height:35px; padding-top:15px; display:block; clear:both;}
.yddownload b a{width:210px; height:35px; line-height:35px; display:block; background:#EC2828; text-align:center; color:#fff; margin:0 auto;}
.yddownload b a:hover{background:#AC0F0F;}
.yddownload dl{clear:both; padding:15px 0 0 0;}
.yddownload dd{float:left; width:88px; height:118px; padding:0 10px;}
.yddownload dd span{height:25px; line-height:25px; padding-top:5px; text-align:center; display:block; font-family:Arial, Helvetica, sans-serif; font-size:14px;}
