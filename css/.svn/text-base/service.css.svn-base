@charset "utf-8";
@import url("rigpost.css");
/* CSS Document */

.iico{background:#999999; color:#fff; height:20px; line-height:20px; margin-left:4px; padding:0 5px; font-size:11px; font-family:"微软雅黑"; display:inline-block;}
.icom{background:url(../images/talent/ico_cx.png) no-repeat; width:16px; height:20px; display:inline-block; line-height:20px;margin-left:2px; padding:0 2px;}
.icocx_on{background-position:5px 1px; width:18px;}/*诚信*/
.icocx_off{background-position:right 1px; width:18px;}
.icosm_on{background-position:2px -26px;}/*实名*/
.icosm_off{background-position:right -26px;}
.icosj_on{background-position:5px -60px;}/*手机*/
.icosj_off{background-position:right -60px;}
.icoyx_on{background-position:0 -91px;}/*邮箱*/
.icoyx_off{background-position:right -91px;}
.icobk_on{background-position:0 -116px;}/*银行卡*/
.icobk_off{background-position:right -116px;}

.bgred{background:#EC2828;}
.bgorange{background:#FF7C0A;}


/*幻灯*/
#flash { POSITION:relative; z-index:1; MARGIN:0px auto; HEIGHT:350px; CLEAR:both; OVERFLOW:hidden; text-align:center; }

#flash A { DISPLAY:none; HEIGHT:350px; }

#flash IMG { HEIGHT:350px }

.flash_bar { POSITION:absolute; TEXT-ALIGN:center; width:100%; BOTTOM:10px; _BOTTOM:30px;OVERFLOW:hidden; margin-right:auto; margin-left:auto; }
.flash_bar .dq { MARGIN:3px 0; WIDTH:15px; DISPLAY:inline-block; BACKGROUND:#ec2828; border-radius:15px; HEIGHT:15px; CURSOR:pointer; }
.flash_bar .no { MARGIN:3px 0; WIDTH:15px; DISPLAY:inline-block; BACKGROUND:#fff; border-radius:10px; HEIGHT:15px; CURSOR:pointer; }

#flashBg { margin:auto; WIDTH:100%; HEIGHT:350px; CLEAR:both; padding:0px; border-bottom:1px solid #dfdfdf; z-index:1; position:relative; }

#flashLine { WIDTH:100%; HEIGHT:350px; CLEAR:both; }

#flshimgload { position:absolute; width:100%; padding-top:145px; text-align:center; z-index:1; }

#flash A img.imgwait { margin:150px auto auto; width:auto; height:auto; }

#flashBg .container { position:relative; }

.task_post_wrap{width:1200px; margin:0 auto; position:relative; z-index:9999;}

.task_post_box{width:200px; height:280px; background:#fff; position:absolute; right:30px; top:-330px; z-index:99; padding:15px;}

.task_post_list{width:200px; height:280px; position:relative; overflow:hidden;}

.task_post_list .jcarousel-clip-horizontal{width:200px;}

.task_post_list .jcarousel-clip-horizontal{width:200px; height:280px;}

.task_post_list ul{width:2000px;  overflow:hidden;}

.task_post_list li{width:200px; float:left;}

.task_post_list em{text-align:center; height:80px; display:block; padding-top:10px;}

.task_post_list h3{font-size:14px; height:35px; line-height:35px;}

.task_post_list span{height:40px; line-height:20px; color:#999; display:block; font-size:12px; padding-bottom:30px;}

.task_post_list a{width:190px; height:35px; line-height:35px; color:#fff; text-align:center; margin:0 auto; display:block; background:#EC2828;}
.task_post_list a:hover{background:#EA5E00;}

.task_post_box .jcarousel-page{ position: absolute; height:20px; top:250px; right:30px; z-index:99999; display:block;width:100px;}

.task_post_box .jcarousel-page a { text-indent:-9999px; display: block; width: 15px; height:15px; overflow:hidden; background: #fff; margin-right: 2px; opacity: .75; border-radius:14px; float:left; padding:0; }

.task_post_box .jcarousel-page a.select{ background:#EC2828;opacity:1;}


/*tabbox*/
.tabbox { border:1px solid #dfdfdf; border-top:none; height:290px; width:940px; margin-right:0; margin-left:auto; background:#fff; }

.tab_tit { height:40px; background:#e9e9e9; }

