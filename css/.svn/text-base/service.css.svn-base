.servicslist_box_img a:hover{border-color:#ec2828;}
.servicslist_box_txt{float:left; width:560px; height:106px; padding-left:20px;}
.servicslist_box_txt h2{height:35px; line-height:35px; font-size:14px; padding-bottom:5px; overflow:hidden;}
.servicslist_box_txt h2 a:link{color:#0066cc;}
.servicslist_box_txt h2 a:hover{color:#ec2828;}
.servicslist_box_txt h2 a:visited {color:#844389;}
.servicslist_box_txt h2 a:active{color:#fd3939;}

.servicslist_box_txt span{height:30px; line-height:30px; display:block;}
.servicslist_box_txt span u{ width:300px; height:30px; overflow:hidden; display:inline-block;}

.servicslist_box_btn{float:right; width:180px; height:86px; position:relative; padding-top:20px;}
.share_button{width:170px; height:90px; position:absolute; left:0; top:30px; z-index:1; display:none;}
.share_button span{height:25px; display:block; padding-top:10px;}
.share_button span.btn a.button{letter-spacing:1px;}
.servicslist_box_btn i{display:none; background:url(../images/task/commend.png) no-repeat right -379px; position:absolute; right:-20px; top:-20px; width:50px; height:50px; z-index:2;}
.sercommend{border:1px solid #efe0cf; background:#fffff4;}
.sercommend .servicslist_box_btn i{display:block;}
.sercommend .servicslist_box_img a{ border-color:#ffffdf;}
.sercommend .servicslist_box_img a:hover{ border-color:#ec2828;}

.commend_tit{background:url(../images/task/bars.png) repeat-x 0 0; border:1px solid #e9e9e9; height:40px; line-height:40px; padding:0 15px;}
.commend_tit h2{float:left; font-size:14px;}
.commend_tit span{float:right;}
.commendlist{padding-top:20px;}
.commendlist_box{width:410px; height:160px; padding:20px; background:#fff; border:1px solid #e9e9e9; float:left;}
.commendlist_box_l{float:left; width:120px; height:160px;}
.commendlist_box_l em{height:120px; display:block;}
.commendlist_box_l span{padding-top:10px; height:25px; line-height:25px; text-align:center; display:block;}
.commendlist_box_r{float:right; width:270px; height:160px; position:relative;}
.commendlist_box_r h2{font-size:14px; height:35px; line-height:35px;}
.commendlist_box_r b{height:35px; line-height:35px; display:block;}
.commendlist_box_r u{height:25px; line-height:25px; display:block; color:#999;}
.commendlist_box_r address{font-style:normal; height:25px; line-height:25px; display:block;}
.commendlist_box_r span{height:25px; line-height:25px; padding-top:8px; display:block;}
.commendlist_box_r span a{margin-right:10px;}
.commendlist_box_r i{background:url(../images/task/commend.png) no-repeat right -379px; width:50px; height:50px; display:block; position:absolute; right:-20px; top:-20px;}
.commendlist_ml{margin-left:16px;}

.endtask{border-top:1px solid #e9e9e9; padding:15px;}
.endtask li{padding:8px 0; line-height:18px;}
.endtask li a, .endtask li b{padding-left:4px;}

.postservice li{float:left; width:70px; height:70px; padding:7px 8px; position:relative;}
.postservice li img{width:70px; height:70px;}
.newcase_box{padding:10px; border-bottom:1px solid #e9e9e9;}
.newcase_box h2{font-size:12px; height:30px; line-height:30px; overflow:hidden;}
.newcase_box span{float:left; width:100px; height:80px; display:block;}
.newcase_box ul{float:right; width:120px; height:80px;}
.newcase_box ul li{height:20px; line-height:20px; overflow:hidden;}
.caseon{border-bottom:none;}

.tooltip{position:absolute; width:320px;}
.bor_on{border-bottom:none;}









