@charset "utf-8";

/*商铺信用等级*/
.ico_bao{background:url(../images/common/ico_bsy.png) no-repeat 0 0; width:18px; height:22px; display:inline-block; vertical-align:middle; margin-right:4px;}
.ico_sou{background:url(../images/common/ico_bsy.png) no-repeat 0 -40px; width:18px; height:22px; display:inline-block; vertical-align:middle; margin-right:4px;}
.ico_yuan{background:url(../images/common/ico_bsy.png) no-repeat 0 -80px; width:18px; height:22px; display:inline-block; vertical-align:middle; margin-right:4px;}
.ico_xing{background:url(../images/shop/xing.gif) no-repeat 0 0; width:23px; height:21px; display:inline-block; vertical-align:middle; margin-right:4px;}
.ico_noxing{background:url(../images/shop/xing.gif) no-repeat 0 bottom; width:23px; height:21px; display:inline-block; vertical-align:middle; margin-right:4px;}

.xycertbox{background:url(../images/shop/bg.gif); width:960px; height:660px; overflow:hidden; padding:20px;}
.xycert_left{float:left; width:440px; padding:270px 20px 0 20px;}
.xycert_left em{width:100px; height:100px; display:block; margin:0 auto;}
.xycert_left h3{text-align:center; font-size:16px; font-family:"微软雅黑"; height:30px; line-height:30px; padding:10px 0 5px 0;}
.xycert_left p{display:block; text-align:center; height:21px; padding:5px 0; font-size:20px; font-family:"微软雅黑"; color:#666;}
.xycert_left h2{text-align:center; font-size:25px; font-family:"微软雅黑"; padding-top:15px;}
.xycert_left h2 font{color:#599804; font-family:Arial, Helvetica, sans-serif;}
.xycert_left span{padding-left:35px; display:block; line-height:18px; color:#666; padding-top:40px;}

.xycert_right{float:right; width:400px; padding:50px 40px 0 40px;}
.xycert_right h3{font-size:15px; line-height:25px; height:25px; font-family:"微软雅黑"; font-weight:normal;}
.xycert_right h4{font-size:15px; line-height:25px; height:25px; font-family:"微软雅黑"; white-space:nowrap; overflow:hidden;}
.xycert_right dl{padding:45px 50px 0 50px; height:50px; display:block;}
.xycert_right dd{float:left; width:50%; text-align:center; color:#666;}
.xycert_right dd span{display:block; line-height:20px;}
.xycert_right dd span.tit{color:#1E5202; font-size:20px; font-family:Arial, Helvetica, sans-serif; line-height:30px;}
.xycert_right ul{padding:20px 40px 0 40px; display:inline-block; color:#666;}
.xycert_right li{padding-bottom:8px; clear:both;}
.xycert_right li samp{width:auto; display:inline-block; border:none; background:none; padding:0; font-size:12px;}
.xycert_right li span{font-size:12px;}
.xycert_right li span a{background:url(../images/common/cert.png) no-repeat right 6px;display:inline-block; padding:0 20px 0 8px; color:#666 !important; margin-right:5px;}
.xycert_right li span a.cert{ background-color:#EEF9DB; border:1px solid #D0E6C4;}
.xycert_right li span a.nocert{ background-color:#f4f4f4; border:1px solid #e9e9e9; background-position:right -24px;}





